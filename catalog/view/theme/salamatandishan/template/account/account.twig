{{ header }}
  {% if success %}
    <div class="uk-alert uk-alert-success uk-margin-remove-bottom uk-text-center uk-text-tiny font">{{ success }}</div>
  {% endif %}
<main data-uk-height-viewport="expand: true">
  <div>
    <div class="uk-padding-large uk-padding-remove-horizontal">
      <div>
        <div id="account-account" class="uk-container">
          <div class="uk-margin-large-bottom">
            <h1 class="uk-text-center uk-text-bold uk-text-secondary uk-margin-small-bottom uk-h2 pageTitle font">{{ heading_title }}</h1>
            <ul class="uk-breadcrumb uk-text-center uk-text-zero uk-margin-remove">
              {% for breadcrumb in breadcrumbs %}
                <li><a class="uk-text-tiny uk-text-muted hover yellow font" href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
              {% endfor %}
            </ul>
          </div>
          <div class="uk-flex-center" data-uk-grid>
            {{ column_left }}
            <div class="uk-width-1-1 uk-width-1-2@m" id="content">
              <div>
                <div class="uk-text-zero" data-uk-grid>
                  {{ content_top }}
                  <div class="uk-width-1-1">
                    <div>
                      <div class="uk-grid-small uk-child-width-1-2 uk-child-width-1-3@m uk-text-center dashboardIcons logged" data-uk-grid>
                        <div>
                          <a href="{{ order }}" title="{{ text_order }}" class="uk-background-muted uk-padding uk-width-1-1 uk-height uk-flex uk-flex-column uk-flex-middle uk-flex-center  uk-border-rounded">
                            <span><img src="catalog/view/theme/salamatandishan/image/sprite.svg#box-open" class="uk-preserve-width" width="32" height="32" alt="{{ text_order }}" data-uk-svg></span>
                            <span class="uk-text-tiny uk-margin-small-top font">{{ text_order }}</span>
                          </a>
                        </div>
                        <div>
                          <a href="{{ address }}" title="{{ text_address }}" class="uk-background-muted uk-padding uk-width-1-1 uk-height uk-flex uk-flex-column uk-flex-middle uk-flex-center uk-border-rounded">
                            <span><img src="catalog/view/theme/salamatandishan/image/sprite.svg#map" class="uk-preserve-width" width="32" height="32" alt="{{ text_address }}" data-uk-svg></span>
                            <span class="uk-text-tiny uk-margin-small-top font">{{ text_address }}</span>
                          </a>
                        </div>
                        <div>
                          <a href="{{ wishlist }}" title="{{ text_wishlist }}" class="uk-background-muted uk-padding uk-width-1-1 uk-height uk-flex uk-flex-column uk-flex-middle uk-flex-center uk-border-rounded">
                            <span><img src="catalog/view/theme/salamatandishan/image/sprite.svg#bookmark" class="uk-preserve-width" width="32" height="32" alt="{{ text_wishlist }}" data-uk-svg></span>
                            <span class="uk-text-tiny uk-margin-small-top font">{{ text_wishlist }}</span>
                          </a>
                        </div>
                        <div>
                          <a href="{{ edit }}" title="{{ text_edit }}" class="uk-background-muted uk-padding uk-width-1-1 uk-height uk-flex uk-flex-column uk-flex-middle uk-flex-center uk-border-rounded">
                            <span><img src="catalog/view/theme/salamatandishan/image/sprite.svg#user" class="uk-preserve-width" width="32" height="32" alt="{{ text_edit }}" data-uk-svg></span>
                            <span class="uk-text-tiny uk-margin-small-top font">{{ text_edit }}</span>
                          </a>
                        </div>
                        <div>
                          <a href="{{ password }}" title="{{ text_password }}" class="uk-background-muted uk-padding uk-width-1-1 uk-height uk-flex uk-flex-column uk-flex-middle uk-flex-center uk-border-rounded">
                            <span><img src="catalog/view/theme/salamatandishan/image/sprite.svg#lock" class="uk-preserve-width" width="32" height="32" alt="{{ text_password }}" data-uk-svg></span>
                            <span class="uk-text-tiny uk-margin-small-top font">{{ text_password }}</span>
                          </a>
                        </div>
                        <div>
                          <a href="https://salamatandishan.net/logout" title="{{ text_logout }}" class="uk-background-muted uk-padding uk-width-1-1 uk-height uk-flex uk-flex-column uk-flex-middle uk-flex-center uk-border-rounded">
                            <span><img src="catalog/view/theme/salamatandishan/image/sprite.svg#signout" class="uk-preserve-width" width="32" height="32" alt="{{ text_logout }}" data-uk-svg></span>
                            <span class="uk-text-tiny uk-margin-small-top font">{{ text_logout }}</span>
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  {{ content_bottom }}
                </div>
              </div>
            </div>
            {{ column_right }}
          </div>
        </div>
      </div>
    </div>
  </div>
</main>
{{ footer }}
{#
<a href="{{ newsletter }}">{{ text_newsletter }}</a>
<a href="{{ download }}">{{ text_download }}</a>
<a href="{{ return }}">{{ text_return }}</a>
<a href="{{ transaction }}">{{ text_transaction }}</a>
<a href="{{ recurring }}">{{ text_recurring }}</a>

{% if reward %}
  <a href="{{ reward }}">{{ text_reward }}</a>
{% endif %}

{% if not tracking %}
  <a href="{{ affiliate }}">{{ text_affiliate_add }}</a>
{% else %}
  <a href="{{ affiliate }}">{{ text_affiliate_edit }}</a>
  <a href="{{ tracking }}">{{ text_tracking }}</a>
{% endif %}

{% if credit_cards %}
  {% for credit_card in credit_cards %}
    <a href="{{ credit_card.href }}">{{ credit_card.name }}</a>
  {% endfor %}
{% endif %}
 #}