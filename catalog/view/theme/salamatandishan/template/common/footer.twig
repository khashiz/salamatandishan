        <div class="uk-padding-small uk-padding-remove-vertical uk-display-inline-block uk-position-relative wrapper">
          <div class="uk-flex-center uk-grid-small" data-uk-grid>
            <div class="uk-width-auto">
              <a href="#" title="" class="uk-display-inline-block"><img src="catalog/view/theme/salamatandishan/image/sprite.svg#instagram" width="40" height="40" data-uk-svg></a>
            </div>
            <div class="uk-width-auto">
              <a href="#" title="" class="uk-display-inline-block"><img src="catalog/view/theme/salamatandishan/image/sprite.svg#telegram" width="40" height="40" data-uk-svg></a>
            </div>
            <div class="uk-width-auto">
              <a href="#" title="" class="uk-display-inline-block"><img src="catalog/view/theme/salamatandishan/image/sprite.svg#whatsapp" width="40" height="40" data-uk-svg></a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="crWrapper uk-padding uk-padding-remove-top uk-padding-remove-horizontal">
    <div>
      <div class="uk-container uk-container-xsmall">
        <div>
          <div data-uk-grid>
            <div class="uk-width-1-1">
              <p class="uk-text-tiny uk-text-muted font">{{ text_copyright }}</p>
            </div>
          </div>
        </div>
      </div>
    </div>
